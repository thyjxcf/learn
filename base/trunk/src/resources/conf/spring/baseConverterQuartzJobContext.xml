<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd"><beans default-lazy-init="true">	<!-- =====处理资源转换状态标识字段（如：正在转换时停了服务，此时状态一直为1，通过下面的定时任务改变其状态）======= -->	<!-- 资源文件 -->	<bean id="resourceFileUpdateStatusJobDetail" class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">		<property name="targetObject">			<ref bean="resourceFileConvertJob" />		</property>		<property name="targetMethod">			<value>updateJobNoHand</value>		</property>	</bean>	<bean id="resourceFileUpdateStatusTrigger" class="org.springframework.scheduling.quartz.CronTriggerBean">		<property name="jobDetail">			<ref bean="resourceFileUpdateStatusJobDetail" />					</property>		<property name="cronExpression">			<value>0 0/50 * * * ? </value>		</property>	</bean>		<bean class="org.springframework.scheduling.quartz.SchedulerFactoryBean" lazy-init="false" autowire="no">		<property name="triggers">			<list>				<ref local="resourceFileUpdateStatusTrigger" />					</list>		</property>	</bean>		<!-- ======文件转换启动定时器（在服务启动1分钟后启动转换任务，转换任务由另外的定时器调度）===== -->	<bean id="resourceFileConvertJob" class="net.zdsoft.leadin.common.job.BusinessTaskQuartzJob" autowire="byName" init-method="init">		<property name="businessTaskService">			<ref bean="resourceFileConvertService" />		</property>	</bean>	<bean id="resourceFileConvertTask" class="org.springframework.scheduling.timer.ScheduledTimerTask">		<property name="timerTask">			<ref bean="resourceFileConvertJob" />		</property>		<property name="delay">			<value>60000</value>		</property>	</bean>		<bean class="org.springframework.scheduling.timer.TimerFactoryBean" lazy-init="false">		<property name="scheduledTimerTasks">			<list>				<ref bean="resourceFileConvertTask" />			</list>		</property>	</bean></beans>