<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd"><beans default-autowire="byName" default-lazy-init="true">	<!-- 文档转换服务 -->	<bean id="documentConvertPropertyPlaceholderConfigurer" class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">		<property name="location">			<value>classpath:documentConvert.properties</value>		</property>	</bean>	<bean id="openOfficeConnection"	class="com.artofsolving.jodconverter.openoffice.connection.SocketOpenOfficeConnection">		<constructor-arg index="0" type="java.lang.String">			<value>${openoffice.host}</value>		</constructor-arg>		<constructor-arg index="1" type="int">			<value>${openoffice.port}</value>		</constructor-arg>	</bean>	<bean id="documentConverter" class="com.artofsolving.jodconverter.openoffice.converter.OpenOfficeDocumentConverter">		<constructor-arg index="0" ref="openOfficeConnection" />	</bean>	<bean id="documentConverterService"	class="net.zdsoft.eis.base.converter.service.impl.DocumentConverterServiceImpl" init-method="init"		destroy-method="destroy">			<property name="connection">			<ref local="openOfficeConnection" />		</property>		<property name="converter">			<ref local="documentConverter" />		</property>	</bean>		<!-- 视频转换服务 -->	<bean id="videoConverterService" class="net.zdsoft.eis.base.converter.service.impl.VideoConverterServiceImpl" init-method="init"		destroy-method="destroy">	</bean>		<!-- windows转换服务 -->	<bean id="windowsConverterFileService" class="net.zdsoft.eis.base.converter.service.impl.WindowsConverterFileServiceImpl" >	</bean>		<!-- openffice转换服务监听（自动重启） -->	<bean id="sofficeListening" class="net.zdsoft.eis.base.converter.listening.SofficeListening" destroy-method="destroy" lazy-init="false">		<property name="isUse" value="${openoffice.isUseListening}" />	</bean>		<!-- 文件转换service -->	<bean id="resourceFileConvertService" class="net.zdsoft.eis.base.converter.service.impl.ResourceFileConvertServiceImpl"/>	<bean id="converterFileService" class="net.zdsoft.eis.base.converter.service.impl.ConverterFileServiceImpl">	    <property name="converterFileDaoMap">			<map>				<entry key="sys_attachment">					<bean class="net.zdsoft.eis.base.converter.dao.impl.ConverterFileDaoImpl">					    <property name="properties">					        <bean class="org.springframework.beans.factory.config.PropertiesFactoryBean">								<property name="location">									<value>classpath:businessconf/attachmentResource.properties</value>								</property>							</bean>					    </property>					</bean>				</entry>			</map>		</property>	</bean>		<bean id="converterFileTypeService" class="net.zdsoft.eis.base.converter.service.impl.ConverterFileTypeServiceImpl"/></beans>